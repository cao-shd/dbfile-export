<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="space.caoshd.dbexpt.mapper.IFileExportTypeTextMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="space.caoshd.dbexpt.po.FileExportTypeTextPO">
        <id column="id" property="id" />
        <result column="charset" property="charset" />
        <result column="line_separator" property="lineSeparator" />
        <result column="field_separator" property="fieldSeparator" />
        <result column="warp_with" property="warpWith" />
        <result column="auto_header" property="autoHeader" />
        <result column="version" property="version" />
        <result column="status" property="status" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="update_by" property="updateBy" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="BaseColumns">
        id,
        charset,
        line_separator,
        field_separator,
        warp_with,
        auto_header,
        version,
        status,
        create_time,
        update_time,
        update_by
    </sql>

</mapper>
